<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <Authors>Ruxo Zheng</Authors>
    <Company>RZ</Company>
    <Description>Foundation library</Description>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <AssemblyVersion>2.0.0.200</AssemblyVersion>
    <FileVersion>2.0.0.200</FileVersion>
    <Version>2.0.0</Version>
    <PackageVersion>2.0.0</PackageVersion>
    <LangVersion>8</LangVersion>
    <Title>RZ.Foundation: Functional and utility functions</Title>
    <PackageLicense>https://opensource.org/licenses/MIT</PackageLicense>
    <PackageTags>utility,functional</PackageTags>
    <PackageReleaseNotes>Fix TextHttp potential socket exhaustion problem</PackageReleaseNotes>
    <Nullable>enable</Nullable>
    <TargetFramework>netstandard2.1</TargetFramework>
    <PackageLicenseUrl>https://opensource.org/licenses/MIT</PackageLicenseUrl>
    <RepositoryUrl>https://github.com/ruxo/foundation</RepositoryUrl>
    <RepositoryType>public</RepositoryType>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <OutputPath>..\..\lib\RZ.Foundation\</OutputPath>
    <DebugSymbols>true</DebugSymbols>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <OutputPath>bin\Debug\netcoreapp2.0\</OutputPath>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
  </ItemGroup>
</Project>