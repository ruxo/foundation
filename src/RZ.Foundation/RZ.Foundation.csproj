<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <Authors>Ruxo Zheng</Authors>
    <Company>RZ</Company>
    <Description>Foundation library</Description>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <LangVersion>9</LangVersion>
    <Title>RZ.Foundation: Functional and utility functions</Title>
    <PackageLicense>https://opensource.org/licenses/MIT</PackageLicense>
    <PackageTags>utility,functional</PackageTags>
    <PackageReleaseNotes>Embrace LanguageExt lib.</PackageReleaseNotes>
    <Nullable>enable</Nullable>
    <PackageLicenseUrl>https://opensource.org/licenses/MIT</PackageLicenseUrl>
    <RepositoryUrl>https://github.com/ruxo/foundation</RepositoryUrl>
    <RepositoryType>public</RepositoryType>
    <TargetFramework>net5.0</TargetFramework>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <OutputPath>../../lib/RZ.Foundation/</OutputPath>
    <DebugSymbols>true</DebugSymbols>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <OutputPath>bin\Debug\netcoreapp2.0\</OutputPath>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="LanguageExt.Core" Version="4.0.3" />
    <PackageReference Include="MinVer" Version="2.5.0">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
  </ItemGroup>
</Project>